{
  "typography": {
    "google-fonts": "enabled",
    "font-family": "Montserrat",
    "self-hosting": "enabled"
  },
  "astroConfigs": {
    "root": "./",
    "srcDir": "./src",
    "publicDir": "./public",
    "outDir": "./dist",
    "site": null,
    "base": "./",
    "trailingSlash": "ignore",
    "format": "directory",
    "host": false,
    "port": 3000,
    "drafts": false,
    "mode": "mdx",
    "theme": "dracula",
    "wrap": true,
    "syntaxHighlight": "shiki",
    "integrations": false,
    "ssr": false
  },
  "spa": {
    "enabled": true
  }
}
